{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aldemeery/enum-polyfill": "^1.0",
        "amrshawky/laravel-currency": "^4.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "codezero/laravel-unique-translation": "^3.4",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.3",
        "fruitcake/laravel-cors": "^2.0",
        "gghughunishvili/entrust": "^4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jackiedo/dotenv-editor": "^1.2",
        "janiskelemen/laravel-setting": "^1.2",
        "kirschbaum-development/eloquent-power-joins": "^2.4",
        "laravel/framework": "^8.65",
        "laravel/helpers": "^1.4",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.6",
        "monarobase/country-list": "^3.2",
        "mostafasewidan/sewidan-field": "dev-master",
        "nwidart/laravel-modules": "^8.2",
        "orangehill/iseed": "^3.0",
        "pragmarx/countries": "^0.8.2",
        "propaganistas/laravel-phone": "^4.3",
        "pusher/pusher-php-server": "^7.0",
        "riverskies/laravel-mobile-detect": "^1.3",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-analytics": "^3.11",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-responsecache": "^6.6",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-translatable": "^4.6",
        "tocaan/aramex": "dev-master",
        "tocaan/fcmfirebase": "dev-master",
        "tocaan/world": "^2.0",
        "unisharp/laravel-filemanager": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/" ,
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds" ,
            "database/seeders"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
